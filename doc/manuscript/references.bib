@article{stocktonmaxwell_carbon_2020,
  title = {Carbon Isotope Ratios in Tree Rings Respond Differently to Climatic Variations than Tree-Ring Width in a Mesic Temperate Forest},
  author = {Stockton Maxwell, R. and Belmecheri, Soumaya and Taylor, Alan H. and Davis, Kenneth J. and Ocheltree, Troy W.},
  year = {2020},
  month = jul,
  volume = {288-289},
  pages = {108014},
  issn = {0168-1923},
  doi = {10.1016/j.agrformet.2020.108014},
  abstract = {Determining the response of forest growth and productivity to climate variability is crucial for understanding and modeling forest carbon sequestration in mesic temperate forests. Most tree-ring analyses have used monthly climate data. Daily climate data may be more appropriate to use in the determination of tree-ring response to climate because cell division, enlargement, and thickening occur during discrete periods of the growing season. We analyze annual tree-ring width and {$\delta$}13C from Tsuga canadensis (1992\textendash 2012) at ten sites in the northeastern US to test the null hypothesis that tree-ring width and {$\delta$}13C respond similarly to critical climate periods using daily climate data and linear regression. There was a significant difference in the climate response of ring width and {$\delta$}13C. Precipitation predicted both {$\delta$}13C and ring width with the highest r2 and greatest level of significance overall. {$\delta$}13C integrated climate over longer periods of the year (and previous year) than ring width at most sites. {$\delta$}13C showed a more consistent seasonal signal across sites for each climate variable than ring width. Our work provides a new perspective on the response of annual tree growth and {$\delta$}13C to climatic variability that can inform ecosystem productivity, carbon cycle, and earth system models.},
  journal = {Agricultural and Forest Meteorology},
  keywords = {Carbon isotopes,Climate response,Critical climate period,Daily climate,Dendrochronology,Productivity,Tree ring,Tsuga canadensis},
  language = {en}
}

@book{stokes_introduction_1968,
  title = {An {{Introduction}} to {{Tree}}-Ring {{Dating}}},
  author = {Stokes, Marvin A. and Smiley, Terah L.},
  year = {1968},
  publisher = {{University of Arizona Press}},
  abstract = {Tree-ring dating, or dendrochronology, is the study of the chronological sequence of annual growth rings in trees. This book--a seminal study in its field--provides a simple yet eloquent introduction to the discipline, explaining what a dendrochronologist does both in the field and in the laboratory. Authors Stokes and Smiley first explain the basic principles of tree-ring dating, then describe details of the process, step by step, from the time a sample is collected until it is incorporated into a master chronology. The book focuses on coniferous evergreens of the Southwest, particularly pi\textendash ons, because they have wide geographic distribution, constitute a large population, and show excellent growth response to certain controlling factors. The book is specifically concerned with the task of establishing a calendar date for a wood or charcoal specimen. This concise but thorough explication of an important discipline will make dendrochonology more meaningful to students and professionals in archaeology, forestry, hydrology, and global change.},
  isbn = {978-0-8165-1680-3},
  keywords = {Gardening / Trees,Nature / Plants / Trees,Science / Life Sciences / Botany},
  language = {en}
}

